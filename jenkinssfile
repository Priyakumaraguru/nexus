@Library('shlib1')_
pipeline{
  agent any
  stages{
  
   stage('Create_Repo')
            {
                steps
                 { 
                    bitbucketconnector()
                    logfunction(" is created")
                 }
             post{
                failure{
                 logfunction(" is not created")
                }
            }
            }
            stage('Collector')
      {
          steps
          {
              bitbucketcollector()
              logfunction("is listed")
          }
          post{
                failure{
                 logfunction(" is not listed")
                }
            }
      }
stage("bambooConnector"){
            steps{
       bambooconnector()
       log("Executed the pipeline Successfully for this project")
      
        }
        post{
        failure{
        log("Not Executed the pipeline Successfully for this project")
        }
        }
        }
       
      
       
        stage("bambooCollector"){
            steps{
       bamboocollector()
        log("Plan listed Successfully for this project")
        }
         post{
        failure{
        log("not listed the plan for this project")
        }
        }
        }
        }
        }
