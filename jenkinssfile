@Library('shlib1')_
pipeline{
  agent any
  stages{
stage("bambooConnector"){
            steps{
       bambooconnector()
       BUILD_STRING = "HTTP/1.1 200 OK"
        if (log.contains(BUILD_STRING)) {
    log("success repo created")
  }
        }
        }
       
      
       
        stage("bambooCollector"){
            steps{
       bamboocollector()
        }
        }
        }
        }
